{
  "name": "Cocos DevTools",
  "version": "1.0",
  "description": "Extends chrome Developer Tools, adding a tab that displays the Cocos2d engine data",
  "devtools_page": "index.html",
  "manifest_version": 2,
  "permissions": [
          "debugger",
          "tabs",
          "activeTab",
          "http://*/*",
          "https://*/*"
        ],
  "icons": {
      "128": "icon_128.png",
      "48" : "icon_48.png"
   },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' 'unsafe-eval'"
}
